class Solution:
    def interpret(self, command: str) -> str:
        i=0
        result=''
        while i<len(command):
            if command[i]=='G':
                result+='G'
            elif command[i]=='(' and command[i+1]==')':
                result+='o'
            elif command[i]=='(' and command[i+1]=='a':
                result+='al'
            else:
                pass
            i=i+1
        return result
                
                
        
        
